}

function airportLocationFromAirportCode(airportCode) {
    var airportName = _.where(AirlinesTimezone, { I: airportCode.toUpperCase() }); if (airportName == "") { airportName = airportCode; }
    else { airportName = airportName[0].C }
    return airportName;
}

async function filterSupplier(leg, supplier, searchUrlwithoutSupplier) {

    var airSupplier = '';
    const defaultSupplier = [2,12,15,7,18,22,23];
    var body = {
        leg: leg,
        supplierCode: supplier.filter(function(e) { return (defaultSupplier.indexOf(e) == -1) }).map(function(e){return e.toString()})
    }

    return await axios.post(clientInfoUrls.b2b.hubRestApi + 'route', body, {
        headers: {
            "Content-Type": "application/json",
            "Authorization": 'Bearer ' + localStorage.id_token
        },
    }).then(function (response) {
        if (response.data) {
            let uniqueSupplierCodes = [];
            response.data.forEach(element => {
                if(response.data.filter((ele)=> ele.supplierCode == element.supplierCode ).length == leg.length && uniqueSupplierCodes.indexOf(Number(element.supplierCode)) == -1){
                    uniqueSupplierCodes.push(Number(element.supplierCode));
                }
            });
            var tmpArr = supplier.filter(function(e) { return (defaultSupplier.indexOf(e) != -1) });
            supplier = [...new Set(uniqueSupplierCodes.concat(tmpArr))];
            
            // const uniqueSupplierCodes = [...new Set(response.data.map(item => Number(item.supplierCode)))];
            // var tmpArr = supplier.filter(function(e) { return (defaultSupplier.indexOf(e) != -1) });
            // supplier = uniqueSupplierCodes.concat(tmpArr);
        }

        if (supplier.length == 0 && response.data.length == 0) {
            alertify.alert('Alert', "Supplier does not support route.");
            oneway.selectedSuppliers = [];
            oneway.loadingprocess = false ;
            multicity.loadingprocess = false ;
            return false;
        }
        if (supplier.length == 0) {
            alertify.alert('Alert', "No supplier selected.");
            oneway.loadingprocess = false ;
            multicity.loadingprocess = false ;
            return false;
        }
        airSupplier = supplier.length > 0 ? supplier.join('|') : '';

        let searchUrl = searchUrlwithoutSupplier + '-' + airSupplier;
        window.location.href = searchUrl;
        
    }).catch(function (error) {
        console.log('Error on Authentication');
    });
}

