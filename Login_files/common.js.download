function getUrlVars() {
    var vars = [],
        hash;
    var newUrl = decodeURIComponent(window.location.href);
    var hashes = newUrl.slice(newUrl.indexOf('?') + 1).split('&');
    for (var i = 0; i < hashes.length; i++) {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
    }
    return vars;
}

function getClientInfo(nodeCode){
    if(nodeCode){
        localStorage.setItem('clientNode', nodeCode.toUpperCase());
        let coreDetails = {
            agency : clientInfoUrls.b2b
        }
        localStorage.setItem('nodeData', JSON.stringify(coreDetails));
    }
}

